  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "/lib",
    "/es",
    "/dist"
  ],
  "scripts": {
    "build": "run-s clean lib dist",
    "clean": "rimraf lib es dist",
    "dev": "webpack-dev-server --config webpack.development.config.js --mode development --open",
    "dist": "webpack --config webpack.config.js --mode production",
    "profile": "webpack --config webpack.config.js --mode production --profile --json > stats.json",
    "lib": "run-p lib:*",
    "lib:cjs": "tsc -p tsconfig.json --target ES5 --module commonjs --outDir lib",
    "lib:es": "tsc -p tsconfig.json --target ES5 --module ESNext --outDir es",
    "test": "jest --coverage"
  },
